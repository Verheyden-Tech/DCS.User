<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
	  <Version>1.0.201.0</Version>
	  <AssemblyVersion>1.0.201.0</AssemblyVersion>
	  <ApplicationVersion>1.2.3.0</ApplicationVersion>
    <ImplicitUsings>enable</ImplicitUsings>
    <Authors>Tobias Verheyden</Authors>
    <Company>Verheyden Tech</Company>
    <Description>Repository database service for DCS.User.</Description>
	  <RootNamespace>DCS.User</RootNamespace>
	  <AssemblyName>DCS.User.Data</AssemblyName>
	  <AssemblyType>Repository</AssemblyType>
    <PackageTags>DCS</PackageTags>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	  <EnableNETAnalyzers>true</EnableNETAnalyzers>
	  <GenerateDocumentationFile>True</GenerateDocumentationFile>
	  <DocumentationFile>Documentation\API.Documentation</DocumentationFile>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageOutputPath>C:\Users\verhe\Desktop\Local.Nuget</PackageOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DCS.CoreLib" Version="1.0.101" />
    <PackageReference Include="DCS.CoreLib.BaseClass" Version="1.0.101" />
    <PackageReference Include="DCS.Data" Version="1.0.101" />
    <PackageReference Include="DCS.LogManager" Version="1.0.5" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\DCS.User\DCS.User.csproj" />
  </ItemGroup>

</Project>
